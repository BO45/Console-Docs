{
  "build": {
    "content":
      [
        {
          "files": ["**/**.md", "**/toc.yml"],
          "exclude": ["**/obj/**", "**/includes/**"]
        }
      ],
    "resource": [
        {
          "files": ["**/images/**","**/**.html"],
          "exclude": ["**/obj/**"]
        }
    ],
    "globalMetadata": {
        "ROBOTS": "INDEX, FOLLOW"
    },
    "externalReference": [
    ],
    "template": "op.html",
    "dest": "opsdocs",
    "fileMetadata": {
      "ms.prod": { "/console/**.yml": "Windows"},
      "ms.technology": { "/console/**.yml": "Console"},
      "manager": {"/console/**.yml": "richturn"}
    }
  }
}
