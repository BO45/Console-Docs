{
  "build": {
    "content":
      [
        {
          "files": ["**/**.md", "**/toc.yml"],
          "exclude": ["**/obj/**", "**/includes/**"]
        }
      ],
    "resource": [
        {
          "files": ["**/images/**","**/**.html"],
          "exclude": ["**/obj/**"]
        }
    ],
    "globalMetadata": {
        "ROBOTS": "INDEX, FOLLOW"
    },
    "externalReference": [
    ],
    "template": "op.html",
    "dest": "opsdocs"
  }
}
